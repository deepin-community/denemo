<?xml version="1.0" encoding="UTF-8"?>
<Denemo>
  <merge>
    <title>A Denemo Keymap</title>
    <author>AT, JRR, RTS</author>
    <map>
      <row type="scheme">
        <action>CursorToMarkedMidiNotePosition</action>
        <after>UnmarkNoteOnset</after>
