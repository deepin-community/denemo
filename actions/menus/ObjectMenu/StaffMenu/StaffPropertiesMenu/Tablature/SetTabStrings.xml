<?xml version="1.0" encoding="UTF-8"?>
<Denemo>
  <merge>
    <title>A Denemo Keymap</title>
    <author>AT, JRR, RTS</author>
    <map>
      <row type="scheme">
        <after>TabStaff</after>
        <action>SetTabStrings</action>
        <_label>Set Open Strings</_label>
        <_tooltip>Replaces the current chord with a Denemo Directive that sets the open strings for a tablature staff
to the notes of that chord.</_tooltip>
      </row>
    </map>
  </merge>
</Denemo>
