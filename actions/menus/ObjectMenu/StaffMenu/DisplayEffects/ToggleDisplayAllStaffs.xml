<?xml version="1.0" encoding="UTF-8"?>
<Denemo>
  <merge>
    <title>A Denemo Keymap</title>
    <author>AT, JRR, RTS</author>
    <map>
      <row type="scheme">
        <action>ToggleDisplayAllStaffs</action>
        <after>ToggleCurrentStaffDisplay</after>
        <_label>Display All (Other) Staffs (Off/On)</_label>
        <_tooltip>Show the current staff in the display and hide/show all other staffs. Typesetting is not affected.</_tooltip>
      </row>
    </map>
  </merge>
</Denemo>
