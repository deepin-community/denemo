<Denemo>
  <merge>
    <title>A Denemo Keymap</title>
    <author>AT, JRR, RTS</author>
    <map>
      <row type="scheme">
        <after>AddLowestUpDiminished5</after>
        <action>AddLowestUpAugmented5</action>
